/* components/searchlistitem/searchItem.wxss */
.search-item {
    display: flex;
    justify-content: flex-start;
    width: 100%;
    height: 112rpx;
    border-bottom: solid 1px #999;
    background-color: #fff;
}

.icon-15 {
    display: inline-block;
    width: 112rpx;
    height: 112rpx;
    color: #000;
}

.item {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
}

.name {
    width: 90%;
    font-size: 16px;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}

.singer {
    font-size: 14px;
    color: #808080;
}